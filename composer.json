    "type": "library",
    "keywords": [
        "ha-framework",
        "ha",
        "elasticsearch"
    ],
    "homepage": "https://github.com/itrnka/ha-elasticsearch-middleware",
    "time": "2017-11-15",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "itrnka",
            "email": "tic@tic.sk",
            "role": "Developer"
        }
    ],
    "support": {},
    "require": {
        "php": "^7.1",
        "itrnka/ha-framework": "^1.5",
        "elasticsearch/elasticsearch": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ha\\Middleware\\NoSQL\\Elasticsearch\\": "src/Elasticsearch"
        }
    }
}